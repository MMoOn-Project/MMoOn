	rdfs:comment "An evaluative property  of a noun that indicates the speaker regards the person or object being referred to with favor or admiration."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "PreferredEvaluative"@eng .
# 
# http://purl.org/linguistics/gold/Prefix

gold:Prefix a owl:Class ;
	rdfs:subClassOf gold:Affix ;
