@prefix : <http://mmoon.org/mmoon/core/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix gold: <http://purl.org/linguistics/gold/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix lexvo: <http://lexvo.org/ontology#> .
@prefix ontolex: <http://www.w3.org/ns/lemon/ontolex#> .

<http://mmoon.org/mmoon/core/> a owl:Ontology ;
	owl:versionIRI <http://mmoon.org/mmoon/core/v1.0.0/> ;
	rdfs:label "Multilingual Morpheme Ontology"@en ;
	dc:title "The Multilingual Morpheme Ontology"@en ;
	dc:identifier "http://mmoon.org/mmoon/core/v1.0.0/"^^xsd:anyURI ;
	owl:versionInfo "Version 1.0, 2016. Created and maintained by Bettina Klimek."@en ;
	dct:license <https://creativecommons.org/licenses/by/4.0/> ;
	dc:description """The Multilingual Morpheme Ontology (MMoOn) is  an ontological model that contains the vocabulary needed to describe the morphological elements, meanings and representations of natural languages. 

This is the reference ontology from which various language specific MMoOn Inventories can be derived."""@en ;
	dc:creator <http://aksw.org/BettinaKlimek.rdf> .
# 
# 
# #################################################################
# #
# #    Annotation properties
# #
# #################################################################
# 
# 
# http://mmoon.org/mmoon/core/definition

:definition a owl:AnnotationProperty ;
	:definition "A definition for MMoOn vocabulary item (classes, properties, also possibly individuals)."@en ;
	rdfs:subPropertyOf rdfs:comment , skos:definition .
# 
# http://mmoon.org/mmoon/core/example

:example a owl:AnnotationProperty ;
	rdfs:subPropertyOf rdfs:comment .
# 
# http://mmoon.org/mmoon/core/externalDefinition

:externalDefinition a owl:AnnotationProperty ;
	:definition """A reference to a (typically alternative) definition for MMoOn vocabulary items. 

Solely to be used to refer to Web Resources identified by their URI/IRI (which ideally should be Linked Data)."""@en ;
	rdfs:subPropertyOf rdfs:comment ;
	rdfs:range rdfs:Resource .
# 
# http://mmoon.org/mmoon/core/externalDefinitionDesc

:externalDefinitionDesc a owl:AnnotationProperty ;
	:definition """A reference to an (typically alternative) definition for MMoOn vocabulary items. 

To be used when no URI/IRI can be provided as
reference point or when additional textual information has to be provided for an URI/IRI reference to be sufficiently specific."""@en ;
	rdfs:subPropertyOf rdfs:comment ;
	rdfs:range xsd:string .
# 
# http://mmoon.org/mmoon/core/usageNote

:usageNote a owl:AnnotationProperty ;
	rdfs:subPropertyOf rdfs:comment .
# 
# http://www.w3.org/2004/02/skos/core#broadMatch

skos:broadMatch a owl:AnnotationProperty .
# 
# http://www.w3.org/2004/02/skos/core#definition

skos:definition a owl:AnnotationProperty .
# 
# 
# 
# #################################################################
# #
# #    Object Properties
# #
# #################################################################
# 
# 
# http://mmoon.org/mmoon/core/associatedToLanguage

:associatedToLanguage a owl:ObjectProperty ;
	rdfs:range :Language ;
	:definition "States that a morpheme inventory, morph or word is associated to a specific langauge."@en ;
	rdfs:label "associated to language"@en .
# 
# http://mmoon.org/mmoon/core/attachedTo

:attachedTo a owl:ObjectProperty ;
	rdfs:domain :Affix ;
	rdfs:range _:genid1 .

_:genid1 a owl:Class ;
	owl:unionOf _:genid3 .

_:genid3 a rdf:List ;
	rdf:first :Root ;
	rdf:rest _:genid2 .

_:genid2 a rdf:List ;
	rdf:first :Stem ;
	rdf:rest rdf:nil .

:attachedTo :definition "States that an affix is attached to some stem or root."@en ;
	rdfs:label "attached to"@en .
# 
# http://mmoon.org/mmoon/core/attachedToRoot

:attachedToRoot a owl:ObjectProperty ;
	rdfs:subPropertyOf :attachedTo ;
	rdfs:domain :Affix ;
	rdfs:range :Root ;
	:definition "States that an affix is attached to some root."@en ;
	rdfs:label "attached to root"@en .
# 
# http://mmoon.org/mmoon/core/attachedToStem

:attachedToStem a owl:ObjectProperty ;
	rdfs:subPropertyOf :attachedTo ;
	rdfs:domain :Affix ;
	rdfs:range :Stem ;
	:definition "States that an affix is attached to some stem."@en ;
	rdfs:label "attached to stem"@en .
# 
# http://mmoon.org/mmoon/core/belongsTo

:belongsTo a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	owl:inverseOf :consistsOfMorph ;
	rdfs:domain :Morph ;
	rdfs:range _:genid4 .

_:genid4 a owl:Class ;
	owl:unionOf _:genid7 .

_:genid7 a rdf:List ;
	rdf:first :Root ;
	rdf:rest _:genid6 .

_:genid6 a rdf:List ;
	rdf:first :Stem ;
	rdf:rest _:genid5 .

_:genid5 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:belongsTo :definition "States that a morph belongs to some word, stem or root."@en ;
	rdfs:label "belongs to"@en .
# 
# http://mmoon.org/mmoon/core/belongsToLexicalEntry

:belongsToLexicalEntry a owl:ObjectProperty ;
	owl:inverseOf :hasWordform ;
	rdfs:domain :Wordform ;
	rdfs:range :LexicalEntry ;
	:definition "States that a word-form resource belongs to a lexical entry resource."@en ;
	rdfs:label "belongs to lexical entry"@en .
# 
# http://mmoon.org/mmoon/core/compoundingRelation

:compoundingRelation a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMorphologicalRelationship ;
	rdfs:domain :CompoundWord ;
	rdfs:range :Compounding ;
	:definition "States which kind of compounding relationship applies to a compound word."@en ;
	rdfs:label "compounding relation"@en .
# 
# http://mmoon.org/mmoon/core/consistsOfAffix

:consistsOfAffix a owl:ObjectProperty ;
	rdfs:subPropertyOf :consistsOfMorph ;
	rdfs:range :Affix ;
	:definition "States that a word, stem or root consists of some affix."@en ;
	rdfs:label "consists of affix"@en .
# 
# http://mmoon.org/mmoon/core/consistsOfMorph

:consistsOfMorph a owl:ObjectProperty ;
	rdfs:domain _:genid8 .

_:genid8 a owl:Class ;
	owl:unionOf _:genid11 .

_:genid11 a rdf:List ;
	rdf:first :Root ;
	rdf:rest _:genid10 .

_:genid10 a rdf:List ;
	rdf:first :Stem ;
	rdf:rest _:genid9 .

_:genid9 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:consistsOfMorph rdfs:range :Morph ;
	:definition "States that a word, stem or root consists of some morph."@en ;
	rdfs:label "consists of morph"@en .
# 
# http://mmoon.org/mmoon/core/consistsOfRoot

:consistsOfRoot a owl:ObjectProperty ;
	rdfs:subPropertyOf :consistsOfMorph ;
	rdfs:range :Root ;
	:definition "States that a word, stem or root consists of some root."@en ;
	rdfs:label "consists of root"@en .
# 
# http://mmoon.org/mmoon/core/consistsOfStem

:consistsOfStem a owl:ObjectProperty ;
	rdfs:subPropertyOf :consistsOfMorph ;
	rdfs:domain _:genid12 .

_:genid12 a owl:Class ;
	owl:unionOf _:genid14 .

_:genid14 a rdf:List ;
	rdf:first :Stem ;
	rdf:rest _:genid13 .

_:genid13 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:consistsOfStem rdfs:range :Stem ;
	:definition "States that a word or stem consists of some stem."@en ;
	rdfs:label "consists of stem"@en .
# 
# http://mmoon.org/mmoon/core/consistsOfWord

:consistsOfWord a owl:ObjectProperty ;
	rdfs:domain _:genid15 .

_:genid15 a owl:Class ;
	owl:unionOf _:genid17 .

_:genid17 a rdf:List ;
	rdf:first :AnalyticWordform ;
	rdf:rest _:genid16 .

_:genid16 a rdf:List ;
	rdf:first :CompoundWord ;
	rdf:rest rdf:nil .

:consistsOfWord rdfs:range :Word ;
	:definition "States that an analytic word-form (such as \"more big\" being the comparative form of the adjective \"big\") or compound word consists of other word resources."@en ;
	rdfs:label "consists of word"@en .
# 
# http://mmoon.org/mmoon/core/contextualInflectionalMeaning

:contextualInflectionalMeaning a owl:ObjectProperty ;
	rdfs:subPropertyOf :inflectionalMeaning ;
	rdfs:range :LinguisticCategory ;
	:definition """States that the meaning \"comprises values (of linguistic categories) which are assigned [...] because of the syntactic context in which it (the word or morph) appears\".
[Haspelmath and Sims: Understanding morphology. 2002:100]"""@en ;
	:example "\"klein-e Frau\" 'small-NOM.SG.F woman.NOM.SG.F' - contextual inflection (noun agreement marking)"@de ;
	rdfs:label "contextual inflectional meaning"@en .
# 
# http://mmoon.org/mmoon/core/correspondsToMorpheme

:correspondsToMorpheme a owl:ObjectProperty ;
	owl:inverseOf :hasRealization ;
	rdfs:domain :Morph ;
	rdfs:range :Morpheme ;
	:definition "States that a morph corresponds to a morpheme."@en ;
	rdfs:label "corresponds to morpheme"@en .
# 
# http://mmoon.org/mmoon/core/derivationalMeaning

:derivationalMeaning a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMeaning ;
	rdfs:range :DerivationalMeaning ;
	:definition "States that a morph, morpheme or word resource has some derivational meaning."@en ;
	rdfs:label "derivational meaning"@en .
# 
# http://mmoon.org/mmoon/core/derivationalRelation

:derivationalRelation a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMorphologicalRelationship ;
	rdfs:domain :DerivedWord ;
	rdfs:range :Derivation ;
	:definition "States which kind of derivational  relationship applies to a derived word."@en ;
	:usageNote "This property is also used to state that a simple lexeme which cannot be further segmented with respect to word-formation has the morphological relationship mmoon:NoWordFormation."@en ;
	rdfs:label "derivational relation"@en .
# 
# http://mmoon.org/mmoon/core/forLanguage

:forLanguage a owl:ObjectProperty ;
	rdfs:subPropertyOf :associatedToLanguage ;
	rdfs:domain :MorphemeInventory ;
	:definition "States that a MMoOn morpheme inventory is specified for a certain language."@en ;
	rdfs:label "for language"@en .
# 
# http://mmoon.org/mmoon/core/grammaticalMeaning

:grammaticalMeaning a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMeaning ;
	rdfs:range :LinguisticCategory , :WordclassAffiliation ;
	:definition "States that a morph, morpheme or word resource has some grammatical meaning."@en ;
	rdfs:label "grammatical meaning"@en .
# 
# http://mmoon.org/mmoon/core/hasAbstractIdentity

:hasAbstractIdentity a owl:ObjectProperty ;
	rdfs:domain _:genid18 .

_:genid18 a owl:Class ;
	owl:unionOf _:genid20 .

_:genid20 a rdf:List ;
	rdf:first :Meaning ;
	rdf:rest _:genid19 .

_:genid19 a rdf:List ;
	rdf:first :Morpheme ;
	rdf:rest rdf:nil .

:hasAbstractIdentity rdfs:range :MorphemicGloss ;
	:definition "States that a morpheme or meaning resource is assigned to one (ore more) morphemic gloss(es)."@en ;
	rdfs:label "has abstract identity"@en .
# 
# http://mmoon.org/mmoon/core/hasMeaning

:hasMeaning a owl:ObjectProperty ;
	rdfs:domain _:genid21 .

_:genid21 a owl:Class ;
	owl:unionOf _:genid24 .

_:genid24 a rdf:List ;
	rdf:first :Morph ;
	rdf:rest _:genid23 .

_:genid23 a rdf:List ;
	rdf:first :Morpheme ;
	rdf:rest _:genid22 .

_:genid22 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:hasMeaning rdfs:range :Meaning ;
	:definition "States that a morph, morpheme or word resource has some meaning."@en ;
	rdfs:label "has meaning"@en .
# 
# http://mmoon.org/mmoon/core/hasMorphemicEntry

:hasMorphemicEntry a owl:ObjectProperty ;
	rdfs:domain :MorphemeInventory ;
	rdfs:range _:genid25 .

_:genid25 a owl:Class ;
	owl:unionOf _:genid27 .

_:genid27 a rdf:List ;
	rdf:first :Morph ;
	rdf:rest _:genid26 .

_:genid26 a rdf:List ;
	rdf:first :Morpheme ;
	rdf:rest rdf:nil .

:hasMorphemicEntry :definition "States that a morpheme inventory resource has morphemes as morphemic entries, thus building the intension of the morpheme inventory."@en ;
	rdfs:label "has morphemic entry"@en .
# 
# http://mmoon.org/mmoon/core/hasMorphologicalRelationship

:hasMorphologicalRelationship a owl:ObjectProperty ;
	rdfs:domain :Word ;
	rdfs:range :MorphologicalRelationship ;
	:definition "Specifies the morphological relationship of a word."@en ;
	rdfs:label "has morphological relationship"@en .
# 
# http://mmoon.org/mmoon/core/hasRealization

:hasRealization a owl:ObjectProperty ;
	rdfs:domain :Morpheme ;
	rdfs:range :Morph ;
	:definition "States that a morpheme is realized by a morph."@en ;
	rdfs:label "has realization"@en .
# 
# http://mmoon.org/mmoon/core/hasRepresentation

:hasRepresentation a owl:ObjectProperty ;
	rdfs:domain _:genid28 .

_:genid28 a owl:Class ;
	owl:unionOf _:genid30 .

_:genid30 a rdf:List ;
	rdf:first :Morph ;
	rdf:rest _:genid29 .

_:genid29 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:hasRepresentation rdfs:range :Representation ;
	:definition "States that a word or morph has some linguistic representation."@en ;
	rdfs:label "has representation"@en .
# 
# http://mmoon.org/mmoon/core/hasSense

:hasSense a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMeaning ;
	rdfs:range :Sense ;
	:definition "States that a morph, morpheme or word resource has some sense."@en ;
	rdfs:label "has sense"@en .
# 
# http://mmoon.org/mmoon/core/hasWordclassAffiliation

:hasWordclassAffiliation a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMeaning ;
	rdfs:range :WordclassAffiliation ;
	:definition "States that a morph, morpheme or word resource has a wordclass affiliation."@en ;
	rdfs:label "has wordclass affiliation"@en .
# 
# http://mmoon.org/mmoon/core/hasWordform

:hasWordform a owl:ObjectProperty ;
	rdfs:domain :LexicalEntry ;
	rdfs:range :Wordform ;
	:definition "States that a lexical entry resource has an associated word-form resource."@en ;
	rdfs:label "has wordform"@en .
# 
# http://mmoon.org/mmoon/core/inflectionalMeaning

:inflectionalMeaning a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMeaning ;
	rdfs:range :LinguisticCategory ;
	:definition "States that a morph, morpheme or word resource has some inflectional meaning."@en ;
	rdfs:label "inflectional meaning"@en .
# 
# http://mmoon.org/mmoon/core/inflectionalRelation

:inflectionalRelation a owl:ObjectProperty ;
	rdfs:subPropertyOf :hasMorphologicalRelationship ;
	rdfs:domain :Word ;
	rdfs:range :Inflection ;
	:definition "States which kind of inflectional relationship applies to a word."@en ;
	rdfs:label "inflectional relation"@en .
# 
# http://mmoon.org/mmoon/core/inherentInflectionalMeaning

:inherentInflectionalMeaning a owl:ObjectProperty ;
	rdfs:subPropertyOf :inflectionalMeaning ;
	rdfs:range :LinguisticCategory ;
	:definition """States that the meaning \"comprises features (= linguistic categories) that are relevant to the syntax but convey a certain amount of independent information\". E.g. the tense/aspect values for verbs or the number or gender values for nouns.
[Haspelmath and Sims: Understanding morphology. 2002:100]"""@de ;
	:example "\"klein-er-e Frau\" 'small-COMPR-NOM.SG.F woman.NOM.SG.F' - inherent inflection for degree of comparison"@de ;
	rdfs:label "inherent inflectionalm eaning"@en .
# 
# http://mmoon.org/mmoon/core/isAllomorphTo

:isAllomorphTo a owl:ObjectProperty , owl:SymmetricProperty ;
	rdfs:domain :Morph ;
	rdfs:range :Morph ;
	:definition "States that a morph is an allomorph to another morph which are both realizations of the same morpheme and which occur in complementary or free distribution."@en ;
	:externalDefinitionDesc "An allomorph is one of two or more complementary morphs which manifest a morpheme in its different phonological or morphological environments. [http://www-01.sil.org/linguistics/GlossaryOflinguisticTerms/WhatIsAnAllomorph.htm]"@en ;
	rdfs:label "is allomorph to"@en .
# 
# http://mmoon.org/mmoon/core/isComposedOf

:isComposedOf a owl:ObjectProperty ;
	rdfs:domain :CompoundWord ;
	rdfs:range _:genid31 .

_:genid31 a owl:Class ;
	owl:unionOf _:genid33 .

_:genid33 a rdf:List ;
	rdf:first :GrammaticalWord ;
	rdf:rest _:genid32 .

_:genid32 a rdf:List ;
	rdf:first :Lexeme ;
	rdf:rest rdf:nil .

:isComposedOf :definition "States that a compound word is composed of a lexical entry resource."@en ;
	rdfs:label "is composed of"@en .
# 
# http://mmoon.org/mmoon/core/isDerivedFrom

:isDerivedFrom a owl:ObjectProperty ;
	rdfs:domain :DerivedWord ;
	rdfs:range _:genid34 .

_:genid34 a owl:Class ;
	owl:unionOf _:genid36 .

_:genid36 a rdf:List ;
	rdf:first :GrammaticalWord ;
	rdf:rest _:genid35 .

_:genid35 a rdf:List ;
	rdf:first :LexicalEntry ;
	rdf:rest rdf:nil .

:isDerivedFrom :definition "States that a derived word is derived from a lexical entry resource."@en ;
	rdfs:label "is derived from"@en .
# 
# http://mmoon.org/mmoon/core/isHomonymTo

:isHomonymTo a owl:ObjectProperty , owl:SymmetricProperty ;
	rdfs:domain :Morph ;
	rdfs:range :Morph ;
	:definition "States that a morph is homonym to another morph, i.e. although sharing the same orthographic and/or morphemic representation both morphs encode different meanings and thus realize different morphemes."@en ;
	rdfs:label "is homonym to"@en .
# 
# http://mmoon.org/mmoon/core/partOfLanguage

:partOfLanguage a owl:ObjectProperty ;
	rdfs:subPropertyOf :associatedToLanguage ;
	rdfs:domain _:genid37 .

_:genid37 a owl:Class ;
	owl:unionOf _:genid39 .

_:genid39 a rdf:List ;
	rdf:first :Morph ;
	rdf:rest _:genid38 .

_:genid38 a rdf:List ;
	rdf:first :Word ;
	rdf:rest rdf:nil .

:partOfLanguage :definition "States that a morph or word resources is a linguistic entity as part of a certain language."@en ;
	rdfs:label "part of language"@en .
# 
# http://mmoon.org/mmoon/core/senseLink

:senseLink a owl:ObjectProperty ;
	rdfs:subPropertyOf owl:topObjectProperty ;
	rdfs:range rdfs:Resource ;
	:definition "This relation points to an external sense resource defining the meaning of a lexeme, morph or morpheme."@en ;
	rdfs:label "sense link"@en .
# 
# http://purl.org/linguistics/gold/acousticRealization

gold:acousticRealization a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:realization ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:SpokenLinguisticExpression ;
	rdfs:comment "The relation between some linguistic unit and its corresponding spoken expression."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "acousticRealization"@eng .
# 
# http://purl.org/linguistics/gold/adjunct

gold:adjunct a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasSyntacticConstituent ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "adjunct"@eng .
# 
# http://purl.org/linguistics/gold/agent

gold:agent a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:semanticRole ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "agent"@eng .
# 
# http://purl.org/linguistics/gold/agrees

gold:agrees a owl:ObjectProperty ;
	rdfs:comment "A relation holding between syntactic units, often manifesting itself in shared form features. NOTE: this could be better defined once syntactic roles and relations are developed."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "agrees"@eng .
# 
# http://purl.org/linguistics/gold/allomorph

gold:allomorph a owl:ObjectProperty ;
	rdfs:comment "The relation that holds between a morpheme and one of its morphs, an occurrence of a morpheme in context."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "allomorph"@eng .
# 
# http://purl.org/linguistics/gold/allophone

gold:allophone a owl:ObjectProperty ;
	rdfs:comment "The relation that holds between a phoneme and one of its phones, an occurrence of a phoneme in context."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "allophone"@eng .
# 
# http://purl.org/linguistics/gold/ancestorVariety

gold:ancestorVariety a owl:ObjectProperty ;
	rdfs:domain gold:HumanLanguageVariety ;
	rdfs:range gold:HumanLanguageVariety ;
	rdfs:comment "ancestorVariety is the predicate expressing the basic diachronic relationship  between a language variety that existed some time in the past and a variety existing at a later time  such that the  former has evolved into the latter through regular language change."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "ancestorVariety"@eng .
# 
# http://purl.org/linguistics/gold/antonym

gold:antonym a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:lexicalRelation ;
	rdfs:comment "antonym"@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "antonym"@eng .
# 
# http://purl.org/linguistics/gold/argument

gold:argument a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:syntacticRole ;
	rdfs:range gold:Clause ;
	rdfs:comment "The syntactic entity about which something is predicated."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "argument"@eng .
# 
# http://purl.org/linguistics/gold/circumscribes

gold:circumscribes a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:morphologicalRelation ;
	rdfs:comment "NOTE: still lacks development. This relation holds between two form units and represents the notion of circumscription in a morphosyntactic system. That is, (circumscribes A B) means that part of A comes before B and part of A comes after B, in the linearization of the units of a language."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "circumscribes"@eng .
# 
# http://purl.org/linguistics/gold/coda

gold:coda a owl:ObjectProperty ;
	rdfs:comment "The closing segment of a syllable."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "coda"@eng .
# 
# http://purl.org/linguistics/gold/complement

gold:complement a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasSyntacticConstituent ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "complement"@eng .
# 
# http://purl.org/linguistics/gold/constituents

gold:constituents a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:comment "The relation holding between a linguistic unit and a list (rdf:Seq) of its constituents."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "constituents"@eng .
# 
# http://purl.org/linguistics/gold/dataStructureRelation

gold:dataStructureRelation a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticDataStructure ;
	rdfs:comment "This subsumes all structuring relations used for LinguisticDataStructures. As a naming convention to distinguish relations in data structure from other relations, all names of dataStructuringRelations begin with 'has-'."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "dataStructureRelation"@eng .
# 
# http://purl.org/linguistics/gold/derivedForm

gold:derivedForm a owl:ObjectProperty ;
	rdfs:domain gold:Stem ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "derivedForm"@eng .
# 
# http://purl.org/linguistics/gold/directObject

gold:directObject a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:object ;
	rdfs:comment "A direct object is a grammatical relation that exhibits a combination of certain independent syntactic properties, such as the following:  the usual grammatical characteristics of the patient of typically  transitive verbs; particular case marking; a particular clause  position; the conditioning of an agreement  affix on the verb; the capability of becoming the clause subject in  passivization; the capability of reflexivization. The identification of the direct object relation may be further confirmed by finding significant overlap with similar direct object relations previously established in other languages. This may be done by analyzing correspondence between  translation equivalents [Crystal 1985, 94; Hartmann and Stork 1972, 155; Comrie 1989, 66; Andrews 1985, 68,120,126; Comrie 1985, 337]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "directObject"@eng .
# 
# http://purl.org/linguistics/gold/entailedBy

gold:entailedBy a owl:ObjectProperty ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "entailedBy"@eng .
# 
# http://purl.org/linguistics/gold/entails

gold:entails a owl:ObjectProperty ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "entails"@eng .
# 
# http://purl.org/linguistics/gold/feature

gold:feature a owl:ObjectProperty ;
	rdfs:range gold:LinguisticProperty ;
	rdfs:comment "The relation between a linguistic unit and a linguistic feature. A feature inheres in its host. NOTE: this relation is distinct from the hasFeature which pertains to data structures."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "feature"@eng .
# 
# http://purl.org/linguistics/gold/follows

gold:follows a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:orderingRelation ;
	rdfs:comment "This relation holds between two linguistic units and represents the inverse of 'precedes'. That is, (follows A B) means that A comes after B in the linearization of the realization of linguistic signs. The inverse of this relation is 'precedes'."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "follows"@eng .
# 
# http://purl.org/linguistics/gold/freeTranslation

gold:freeTranslation a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:translation ;
	rdfs:comment "The relation between an orthographic expression in one language and some orthographic expression in another such that both expressions have exactly the same meaning. The words in the translation may not correspond to the those in the source expression."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "freeTranslation"@eng .
# 
# http://purl.org/linguistics/gold/geneticallyRelated

gold:geneticallyRelated a owl:ObjectProperty ;
	rdfs:domain gold:HumanLanguageVariety ;
	rdfs:range gold:HumanLanguageVariety ;
	rdfs:comment "geneticallyRelated is the basic kinship relation between languages varieties. If two language varieties are genetically related, then this implies that both  varieties are derived from a common  proto-language."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "geneticallyRelated"@eng .
# 
# http://purl.org/linguistics/gold/goal

gold:goal a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:semanticRole ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "goal"@eng .
# 
# http://purl.org/linguistics/gold/hasComplexValue

gold:hasComplexValue a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:ComplexSpecification ;
	rdfs:range gold:FeatureStructure ;
	rdfs:comment "This relates a ComplexSpecification to a FeatureStructure, thus giving a FeatureStructure its recursive properties."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasComplexValue"@eng .
# 
# http://purl.org/linguistics/gold/hasConstituent

gold:hasConstituent a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:LinguisticUnit ;
	rdfs:comment "The relation hasConstituent is an abstract partial ordering relation that expresses dominance between two linguistic units. This relation is meant to cover all possible types of linguistic constituency: phonological, morphological, syntactic, etc. constituency."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasConstituent"@eng .
# 
# http://purl.org/linguistics/gold/hasConstraint

gold:hasConstraint a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:FeatureSystem ;
	rdfs:range gold:FeatureConstraint ;
	rdfs:comment "This relates a FeatureSystem to a FeatureContraint."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasConstraint"@eng .
# 
# http://purl.org/linguistics/gold/hasFeature

gold:hasFeature a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:FeatureSpecification ;
	rdfs:comment "This relates a FeatureSpecification to a type of LinguisticProperty."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasFeature"@eng .
# 
# http://purl.org/linguistics/gold/hasForm

gold:hasForm a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticSign ;
	rdfs:range gold:FormUnit ;
	rdfs:comment "The relation that associates some LinguisticSign with its FormUnit."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasForm"@eng .
# 
# http://purl.org/linguistics/gold/hasGlosses

gold:hasGlosses a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:comment "The binary relation holding between an instance of interlinear glossed text (IGT) and a sequence of labels or 'grams' used to describe the morphemes of the IGT."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasGlosses"@eng .
# 
# http://purl.org/linguistics/gold/hasGrammar

gold:hasGrammar a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticSign ;
	rdfs:range gold:GrammarUnit ;
	rdfs:comment "The relation that holds between a LinguisticSign and a GrammaticalUnit."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasGrammar"@eng .
# 
# http://purl.org/linguistics/gold/hasLexicalItem

gold:hasLexicalItem a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:Lexicon ;
	rdfs:range gold:LexicalItem ;
	rdfs:comment "The relation between a Lexicon and its contents, instances of LexicalItem. NOTE: this could probably be replaced by the memberOf relation from set theory."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasLexicalItem"@eng .
# 
# http://purl.org/linguistics/gold/hasLexicalUnit

gold:hasLexicalUnit a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:LexicalItem ;
	rdfs:comment "This relates a LexicalItem to a LexicalUnit, those elements commonly represented in a dictionary."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasLexicalUnit"@eng .
# 
# http://purl.org/linguistics/gold/hasMeaning

gold:hasMeaning a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticSign ;
	rdfs:range gold:SemanticUnit ;
	rdfs:comment "The relation that associates some LinguisticSign with a SemanticUnit."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasMeaning"@eng .
# 
# http://purl.org/linguistics/gold/hasMorphologicalConstituent

gold:hasMorphologicalConstituent a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasConstituent ;
	rdfs:domain gold:MorphologicalUnit ;
	rdfs:range gold:MorphologicalUnit ;
	rdfs:comment "The relation hasMorphologicalConstituent is an abstract partial ordering relation that expresses dominance between two morphological units. This relation is meant to cover all possible types of morphological constituency."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasMorphologicalConstituent"@eng .
# 
# http://purl.org/linguistics/gold/hasProperty

gold:hasProperty a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticSign ;
	rdfs:range gold:LinguisticProperty ;
	rdfs:comment "The relation between a linguistic unit and a linguistic property, e.g., (m1 hasProperty AllativeCase) where m1 is an instance of Morpheme."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasProperty"@eng .
# 
# http://purl.org/linguistics/gold/hasSourceLine

gold:hasSourceLine a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:comment "The binary relation holding between an instance of interlinear glossed text and a linguistic unit (clause, phrase, etc.) from the source language."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasSourceLine"@eng .
# 
# http://purl.org/linguistics/gold/hasSpecification

gold:hasSpecification a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:FeatureStructure ;
	rdfs:range gold:FeatureSpecification ;
	rdfs:comment "This relates a FeatureStructure to a FeatureSpecification."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasSpecification"@eng .
# 
# http://purl.org/linguistics/gold/hasSyntacticConstituent

gold:hasSyntacticConstituent a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasConstituent ;
	rdfs:domain gold:SyntacticUnit ;
	rdfs:range gold:SyntacticUnit ;
	rdfs:comment "The relation hasSyntacticConstituent is an abstract partial ordering relation that expresses dominance between two syntactic units. This relation is meant to cover all possible types of syntactic constituency."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasSyntacticConstituent"@eng .
# 
# http://purl.org/linguistics/gold/hasTerm

gold:hasTerm a owl:ObjectProperty ;
	rdfs:domain gold:Termset ;
	rdfs:range gold:Term ;
	rdfs:comment "The relation between some Termset and a Term."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasTerm"@eng .
# 
# http://purl.org/linguistics/gold/hasTranslationLine

gold:hasTranslationLine a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:comment "The binary relation holding between an instance of interlinear glossed text and a linguistic unit (clause, phrase, etc.) acting as a free translation of the source text from the IGT instance."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasTranslationLine"@eng .
# 
# http://purl.org/linguistics/gold/hasType

gold:hasType a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:dataStructureRelation ;
	rdfs:domain gold:FeatureStructure ;
	rdfs:comment "This relates either a FeatureStructure or a FeatureConstraint to its type, expressed by an instance of PartOfSpeech."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hasType"@eng .
# 
# http://purl.org/linguistics/gold/head

gold:head a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasSyntacticConstituent ;
	rdfs:range gold:Phrase ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "head"@eng .
# 
# http://purl.org/linguistics/gold/hypernym

gold:hypernym a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:lexicalRelation ;
	rdfs:comment "hypernym"@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "hypernym"@eng .
# 
# http://purl.org/linguistics/gold/inLanguage

gold:inLanguage a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:HumanLanguageVariety ;
	rdfs:comment "The relation that holds between a LinguisticSign and a Language. The sign is said to be a member or part of that language."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "inLanguage"@eng .
# 
# http://purl.org/linguistics/gold/indirectObject

gold:indirectObject a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:object ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "indirectObject"@eng .
# 
# http://purl.org/linguistics/gold/infixedIn

gold:infixedIn a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:morphologicalRelation ;
	rdfs:comment "infixedIn is the relation between a Lexical- or SublexicalUnit and a Root. The Root is realized as discontinuous, surrounding the inserted Lexical- or SublexicalUnit [Hartmann and Stork 1972, 111]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "infixedIn"@eng .
# 
# http://purl.org/linguistics/gold/inflectedForm

gold:inflectedForm a owl:ObjectProperty ;
	rdfs:domain gold:Stem ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "inflectedForm"@eng .
# 
# http://purl.org/linguistics/gold/labels

gold:labels a owl:ObjectProperty ;
	rdfs:comment "This relation names or simply associates some SymbolicString with any Entity."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "labels"@eng .
# 
# http://purl.org/linguistics/gold/lexicalRelation

gold:lexicalRelation a owl:ObjectProperty ;
	rdfs:comment "This is the superclass of common lexical relatations such as synonym, antonym, etc. NOTE: this needs work. Such relations really pertain to meaning and not form units."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "lexicalRelation"@eng .
# 
# http://purl.org/linguistics/gold/literalTranslation

gold:literalTranslation a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:translation ;
	rdfs:comment "The relation between an orthographic expression in one language and some orthographic expression in another such that the translation is done on a word by word, or morpheme by morpheme, basis without regard for idiomatic usage."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "literalTranslation"@eng .
# 
# http://purl.org/linguistics/gold/meronym

gold:meronym a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:lexicalRelation ;
	rdfs:comment "meronym"@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "meronym"@eng .
# 
# http://purl.org/linguistics/gold/morphologicalRelation

gold:morphologicalRelation a owl:ObjectProperty ;
	rdfs:domain gold:MorphologicalUnit ;
	rdfs:range gold:MorphologicalUnit ;
	rdfs:comment "A relation holding between morphological units."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "morphologicalRelation"@eng .
# 
# http://purl.org/linguistics/gold/mutuallyIntelligible

gold:mutuallyIntelligible a owl:ObjectProperty ;
	rdfs:domain gold:HumanLanguageVariety ;
	rdfs:range gold:HumanLanguageVariety ;
	rdfs:comment "mutuallyIntelligible is the binary, symmetric relation holding between two language varieties such that speakers of the first variety can communicate with members of the second with relative ease, and vice versa."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "mutuallyIntelligible"@eng .
# 
# http://purl.org/linguistics/gold/names

gold:names a owl:ObjectProperty ;
	rdfs:domain gold:WrittenLinguisticExpression ;
	rdfs:comment "This semiotic relation associates some OrthographicExpression with some Entity. It differs from 'labels' in that a name is usually considered part of the orthographic system, where a label is not."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "names"@eng .
# 
# http://purl.org/linguistics/gold/nucleus

gold:nucleus a owl:ObjectProperty ;
	rdfs:comment "The central segment of a syllable."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "nucleus"@eng .
# 
# http://purl.org/linguistics/gold/object

gold:object a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:argument ;
	rdfs:comment "An object, traditionally defined, is either a direct object or an indirect object.An object, in some usages, is any  grammatical relation other than subject [Crystal 1985, 211; Hartmann and Stork 1972, 155-156; Comrie 1989, 66]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "object"@eng .
# 
# http://purl.org/linguistics/gold/onset

gold:onset a owl:ObjectProperty ;
	rdfs:comment "The initial segment of a syllable."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "onset"@eng .
# 
# http://purl.org/linguistics/gold/orderingRelation

gold:orderingRelation a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:LinguisticUnit ;
	rdfs:comment "Any relation that establishes a linear ordering of linguistic units."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "orderingRelation"@eng .
# 
# http://purl.org/linguistics/gold/parentVariety

gold:parentVariety a owl:ObjectProperty ;
	rdfs:domain gold:HumanLanguageVariety ;
	rdfs:range gold:HumanLanguageVariety ;
	rdfs:comment "parentVariety is the  relation of direct genetic relatedness, where there are  no intermediate ancestors between the ancestor and descendant. For example, Old English is the parent variety of Middle English."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "parentVariety"@eng .
# 
# http://purl.org/linguistics/gold/patient

gold:patient a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:semanticRole ;
	rdfs:comment "A semantic role, often unmarked, that typically does not act with volition, instigate an event, receive something, or experience a sensory impression.  A prototypical patient undergoes a physical, visible change in state. Often the subject of a intransitive verb or the logical complement of a transitive verb. [Payne 1997: 50-51; Pei "@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "patient"@eng .
# 
# http://purl.org/linguistics/gold/precedes

gold:precedes a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:orderingRelation ;
	rdfs:comment "This relation holds between two linguistic units and represents the notion of precedence in a language. That is, (precedes A B) means that A comes before B in the linearization of the realization of linguistic signs. This inverse of this relation is 'follows'."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "precedes"@eng .
# 
# http://purl.org/linguistics/gold/predicate

gold:predicate a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:syntacticRole ;
	rdfs:range gold:Clause ;
	rdfs:comment "The predicate is the relation between the Clause and a portion of a  clause, excluding the subject, that expresses something about the subject [Crystal 1980, 280; Hartmann and Stork 1972, 182; Pei and Gaynor 1954, 173; Pike and Pike 1982, 40; Crystal 1985, 241-242]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "predicate"@eng .
# 
# http://purl.org/linguistics/gold/prefix

gold:prefix a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:morphologicalRelation ;
	rdfs:comment "The relation between a morphological unit and the  lexical unit to which it is attached. The LexicalUnit is usually a Root or Stem. The inverse of prefix is suffix [Crystal 1980, 281; Hartmann and Stork 1972, 182]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "prefix"@eng .
# 
# http://purl.org/linguistics/gold/realization

gold:realization a owl:ObjectProperty ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:LinguisticExpression ;
	rdfs:comment "The relation holding between a linguistic unit and its physical form, either a spoken, written or signed expression."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "realization"@eng .
# 
# http://purl.org/linguistics/gold/rhyme

gold:rhyme a owl:ObjectProperty ;
	rdfs:comment "The core of a syllable, consisting of a nucleus and coda."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "rhyme"@eng .
# 
# http://purl.org/linguistics/gold/semanticRole

gold:semanticRole a owl:ObjectProperty ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "semanticRole"@eng .
# 
# http://purl.org/linguistics/gold/signedRealization

gold:signedRealization a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:realization ;
	rdfs:domain gold:LinguisticUnit ;
	rdfs:range gold:SignedLinguisticExpression ;
	rdfs:comment "The relation between some linguistic unit and its corresponding signed expression."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "signedRealization"@eng .
# 
# http://purl.org/linguistics/gold/specifier

gold:specifier a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:hasSyntacticConstituent ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "specifier"@eng .
# 
# http://purl.org/linguistics/gold/subject

gold:subject a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:argument ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "subject"@eng .
# 
# http://purl.org/linguistics/gold/suffix

gold:suffix a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:morphologicalRelation ;
	rdfs:comment "The relation between a morphological unit and the  lexical unit to which it is attached. The LexicalUnit is usually a Root or Stem. The inverse of suffix is 'prefix' [Crystal 1980, 340; Hartmann and Stork 1972, 226]."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "suffix"@eng .
# 
# http://purl.org/linguistics/gold/synonym

gold:synonym a owl:ObjectProperty ;
	rdfs:subPropertyOf gold:lexicalRelation ;
	rdfs:comment "synonym"@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "synonym"@eng .
# 
# http://purl.org/linguistics/gold/syntacticRole

gold:syntacticRole a owl:ObjectProperty ;
	rdfs:comment """A general category subsuming relations relevant at the level of the Clause, such as predicate and subject. 
A grammatical relation is a role of a phrase or  complement clause that determines syntactic behaviors such as the following: word position in a clause; verb agreement; participation and behavior in such operations as  passivization [Comrie 1989, 65-66; Andrews 1985, 66]."""@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
	rdfs:label "syntacticRole"@eng .
# 
# http://purl.org/linguistics/gold/translation

gold:translation a owl:ObjectProperty ;
	rdfs:comment "The relation between an orthographic expression in one language and some orthographic expression in another such that both expressions have the same or roughly the same meaning."@eng ;
	rdfs:isDefinedBy "http://purl.org/linguistics/gold"^^xsd:anyURI ;
